{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "api"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ]
}
